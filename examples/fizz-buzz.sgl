(program
    (for-each x (range 1 100 1)
        (if (and (== (% x 3) 0) (== (% x 5) 0))
            (print-line "FizzBuzz")
            (if (== (% x 3) 0)
                (print-line "Fizz")
                (if (== (% x 5) 0)
                    (print-line "Buzz")
                    (print-line x)
                )
            )
        )
    )
)