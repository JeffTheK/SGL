(program
    (for-each x (range 1 100 1)
        (if (and (== (% x 3) 0) (== (% x 5) 0))
            (print "FizzBuzz")
            (if (== (% x 3) 0)
                (print "Fizz")
                (if (== (% x 5) 0)
                    (print "Buzz")
                    (print x)
                )
            )
        )
    )
)